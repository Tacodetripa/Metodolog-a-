Algoritmo saludar
	//desarrollado por brayan palma aboytes
	//05/12/22
	
	//INICIO
	definir saludo como entero;
	//DESCRIPCION
	escribir "Algoritmo que realiza saludos 10 veces";
	escribir "--------------------------------------";
	//Proceso 
	Mientras saludo <= 10
		escribir "Hola BOBIIIIIS";
		saludo = saludo +1;
	FinMientras
	
	
FinAlgoritmo
